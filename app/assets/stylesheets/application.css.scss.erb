.pagination_wrap{
	
	position:relative;
	margin-bottom:15px;
	width:100%;
	
	div.pagination{
		text-align:center;
		
		span.page{
			text-align:center;
			display:inline-block;
			
			a{
				display:inline-block;
				background-color:#F1F1F1;
				border:1px solid #ccc;
				
				height:23px;
				line-height:23px;
				min-width:22px;
				text-align:center;
				
				border-radius: 					4px;
				-moz-border-radius: 		4px;
				-webkit-border-radius:	4px;
				
			}
			
			a:hover{
				background-color:#3FA9F5;
				border:1px solid #3FA9F5;
				color:white;
			}
		}
		
		span.current{
			display:inline-block;
			
			background-color:#3FA9F5;
			border:1px solid #3FA9F5;
			color:white;	
			
			height:23px;
			line-height:23px;
			min-width:22px;
			text-align:center;
			
			border-radius: 					4px;
			-moz-border-radius: 		4px;
			-webkit-border-radius:	4px;					
		}
		
		span.prev,
		span.next{
			top:0px;
			position:absolute;
			display:inline-block;
			a{
				background-repeat:no-repeat;
				height:23px;
				line-height:23px;
				
			}
		}
		
		span.prev{
			left:0px;
			a{
				background-image: url("<%= asset_path 'prev.png' %>");
				background-position: center left;
				padding-left:10px;
			}
		}
		
		span.next{
			right:10px;
			a{
				background-image: url("<%= asset_path 'next.png' %>");
				background-position: center right;
				padding-right:10px;
			}
		}
		
		
		span.first,
		span.last,
		span.gap{
			display:none;
		}
	}	
}

